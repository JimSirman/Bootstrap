{
  "name": "confusion",
  "version": "1.0.0",
  "description": "This is a website for Ristorante Con Fusion",
  "main": "index.html",
  "scripts": {
    "start": "npm run watch:all",
    "startLite": "npm run lite",
    "test": "echo \"Error: no test specified\" && exit 1",
