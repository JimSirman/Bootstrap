    "onchange": "^4.1.0",
    "parallelshell": "^3.0.2",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.5.1"
  },
  "dependencies": {
    "bootstrap": "^4.0.0",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "hoek": "^5.0.4",
    "jquery": "^3.1.1",
    "popper.js": "^1.12.9",
    "request-promise": "^4.2.2",
